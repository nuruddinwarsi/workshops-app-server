<%- include( "partials/head", { title: 'Workshops' } ) -%>
<h1>List of workshops</h1>
<hr />
<div>
  <% if( workshops.length === 0 ) { %>
  <p>No workshops to show</p>
  <% } %>
  <ul>
    <% workshops.forEach(workshop => { %>
    <li><%= workshop.name %></li>
    <% }); %>
  </ul>
</div>
<%- include( "partials/foot" ) -%>
